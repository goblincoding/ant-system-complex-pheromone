<Window x:Class="AntSimComplexUI.Dialogs.DirectoryBrowserDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AntSimComplexUI.UserControls"
        mc:Ignorable="d"
        Title="Set path to TSPLIB95" Height="75" Width="500"
        ResizeMode="NoResize" WindowStartupLocation="CenterOwner">
    <Grid x:Name="LayoutRoot">
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <local:DirectoryBrowserControl x:Name="dirBrowser" Grid.Row="0" />
    </Grid>
</Window>